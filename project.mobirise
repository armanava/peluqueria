n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"bg.type!='color' && overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container align-center mbr-white\">\n        <div class=\"title-wrap\" mbr-if=\"showTitle || showSubtitle\">\n            <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .input-group-btn, .underline\">\n                Reservas</h1>\n            <div class=\"underline\" mbr-if=\"showUnderline && showTitle\">\n                <div class=\"line\"></div>\n            </div>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-light\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .input-group-btn\">\n                Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n            </h2>\n        </div>\n\n        <div class=\"mbr-row mbr-justify-content-center\">\n            <div mbr-form class=\"mbr-col-sm-12 mbr-col-md-10 mbr-col-lg-6\">\n                <form class=\"mbr-form\" action=\"info@armanava.com\" method=\"post\" name=\"Mobirise Form\" target=\"_top\" novalidate>\n                    <div submit-success>\n                        <template data-form-alert type=\"amp-mustache\">Su Reserva se ha hecho co éxcitol!</template>\n                    </div>\n                    <div submit-error>\n                        <template data-form-alert type=\"amp-mustache\">Su Reserva se ha hecho co éxcitol!</template>\n                    </div>\n                    <div class=\"fieldset\">\n                        <div class=\"split-row\">\n                            <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Name\">\n                            <input type=\"text\" name=\"form[data][0][1]\" data-form-field=\"Name\" placeholder=\"Nombre\" required>\n                            \n                            <input type=\"hidden\" name=\"form[data][1][0]\" value=\"Phone\">\n                            <input type=\"tel\" name=\"form[data][1][1]\" data-form-field=\"Phone\" placeholder=\"Movíl\" required>\n                        </div>\n\n                        <input type=\"hidden\" name=\"form[data][2][0]\" value=\"Email\">\n                        <input type=\"email\" name=\"form[data][2][1]\" data-form-field=\"Email\" placeholder=\"Email\" required>\n                        \n                        <div class=\"split-row\">\n                            <input type=\"hidden\" name=\"form[data][3][0]\" value=\"Date\">\n                            <input type=\"date\" class=\"datepicker\" min=\"{{minDate}}\" max=\"{{maxDate}}\" name=\"form[data][3][1]\" data-form-field=\"Date\" placeholder=\"Date\" value=\"{{minDate}}\" required>\n                            \n                            <input type=\"hidden\" name=\"form[data][4][0]\" value=\"Time\">\n                            <input type=\"time\" class=\"timepicker\" min=\"{{minTime}}\" max=\"{{maxTime}}\" step=\"{{60*timeStep}}\" name=\"form[data][4][1]\" data-form-field=\"Time\" placeholder=\"Time\" value=\"{{minTime}}\" required>\n                        </div>\n\n                        <input type=\"hidden\" name=\"form[data][5][0]\" value=\"Service Name\">\n                        <input type=\"text\" name=\"form[data][5][1]\" data-form-field=\"Service Name\" placeholder=\"Nombre del Servicio\" required>\n\n                    </div>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" data-toolbar=\"-mbrLink,-mbrBtnAdd,-mbrBtnMove\" class=\"input-group-btn align-center\"><a type=\"submit\" class=\"btn btn-primary btn-form\" data-app-placeholder=\"Type Text\">ENVIAR</a></div>\n                </form>\n            </div>\n        </div>\n    </div>\n</se